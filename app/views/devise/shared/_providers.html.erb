<%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
      <%- if provider.to_s.include?("google") %>
        <%= link_to "Sign in with Google", omniauth_authorize_path(resource_name, provider), class: 'btn btn-block btn-primary outline' %><br />
      <%- else %>
        <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'btn btn-primary outline' %><br />
      <%- end %>
    <% end -%>
<% end -%>
